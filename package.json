{
  "name": "react-formation",
  "version": "1.0.0",
  "description": "TPs de la formation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --progress",
    "test": "jest",
    "test:w": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbcazaux/reactjs-formation.git"
  },
  "author": "jbcazaux",
  "devDependencies": {
    "@types/enzyme": "2.7.4",
    "@types/es6-shim": "0.31.32",
    "@types/jest": "18.1.1",
    "@types/react": "15.0.11",
    "@types/react-bootstrap": "0.0.45",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.36",
    "@types/redux": "3.6.31",
    "@types/redux-thunk": "2.1.31",
    "enzyme": "2.7.1",
    "es6-shim": "0.35.3",
    "html-webpack-plugin": "2.28.0",
    "jest": "18.1.0",
    "react-addons-test-utils": "15.4.2",
    "source-map-loader": "0.1.6",
    "ts-jest": "18.0.3",
    "ts-loader": "2.0.0",
    "typescript": "2.1.6",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "dependencies": {
    "axios": "0.15.3",
    "react": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "testPathIgnorePatterns": ["/node_modules/", "/__tests__/__mocks__/"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__tests__/__mocks__/file-mock.ts"
    }
  }
}
